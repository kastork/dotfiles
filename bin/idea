#!/bin/bash
#Generated by JetBrains Toolbox 1.27.3.14493 at 2023-03-29T09:05:10.206161

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/kirk/Library/Application Support/JetBrains/Toolbox/apps/IDEA-E/ch-0/222.4345.35/IntelliJ IDEA Edu.app/Contents/MacOS/idea" $wait --args "${ideargs[@]}"