#!/bin/bash
#Generated by JetBrains Toolbox 1.27.3.14493 at 2023-03-29T09:05:10.198233

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/kirk/Library/Application Support/JetBrains/Toolbox/apps/PhpStorm/ch-0/223.8836.42/PhpStorm.app/Contents/MacOS/phpstorm" $wait --args "${ideargs[@]}"