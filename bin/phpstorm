#!/bin/bash
#Generated by JetBrains Toolbox 1.25.12627 at 2022-08-08T12:12:52.655768

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/kirk/Library/Application Support/JetBrains/Toolbox/apps/PhpStorm/ch-0/222.3345.135/PhpStorm.app/Contents/MacOS/phpstorm" $wait --args "${ideargs[@]}"