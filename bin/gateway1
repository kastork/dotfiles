declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/kirk/Library/Application Support/JetBrains/Toolbox/apps/Gateway/ch-1/223.8617.56/JetBrains Gateway.app/Contents/MacOS/gateway" $wait --args "${ideargs[@]}"